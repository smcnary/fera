// array_sum.fera - Sum array elements

i32 sum_array(const i32* arr, usize len) {
    i32 total = 0;
    for (usize i = 0; i < len; i = i + 1) {
        total = total + arr[i];
    }
    return total;
}

export i32 main() {
    i32 numbers[5];
    numbers[0] = 1;
    numbers[1] = 2;
    numbers[2] = 3;
    numbers[3] = 4;
    numbers[4] = 5;
    
    i32 result = sum_array(numbers, 5);
    return 0;
}

