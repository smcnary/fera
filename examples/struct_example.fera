// struct_example.fera - Struct usage example

struct Vec3 {
    f32 x;
    f32 y;
    f32 z;
};

Vec3 vec3_new(f32 x, f32 y, f32 z) {
    Vec3 v;
    v.x = x;
    v.y = y;
    v.z = z;
    return v;
}

f32 vec3_dot(Vec3 a, Vec3 b) {
    return a.x * b.x + a.y * b.y + a.z * b.z;
}

export i32 main() {
    Vec3 v1 = vec3_new(1.0f, 2.0f, 3.0f);
    Vec3 v2 = vec3_new(4.0f, 5.0f, 6.0f);
    
    f32 dot = vec3_dot(v1, v2);
    
    return 0;
}

